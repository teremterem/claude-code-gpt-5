OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Recommended: keep this `true` (default).
# Claude Code CLI cannot handle multiple tool calls in a single
# model response, which GPT-5 with medium/high reasoning may attempt.
GPT_ENFORCE_ONE_TOOL_CALL_PER_RESPONSE=true

# OPTIONAL: Langfuse configuration for logging all LiteLLM requests/responses. Useful for debugging.
# If provided, Langfuse needs to be installed with either `uv sync --extra langfuse` or `uv sync --all-extras`.
#LANGFUSE_SECRET_KEY="sk-..."
#LANGFUSE_PUBLIC_KEY="pk-..."
#LANGFUSE_HOST="https://cloud.langfuse.com"
